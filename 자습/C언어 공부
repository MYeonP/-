# C언어_01

#include <stdio.h>

int main(void)
{
    puts(" * 한 문자 출력 * ");
    putchar('A');
    putchar('B');
    putchar('\t');
    putchar('a');
    putchar('b');
    putchar('\n');
    putchar('0');
    putchar('1');
    putchar('\n');
    
    puts("putchr 함수는 한 문자 출력 전용 함수");
    
    return 0;
}

